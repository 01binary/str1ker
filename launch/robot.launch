<launch>
  <node name="robot" pkg="str1ker" type="robot" required="true" output="screen">
    <rosparam>
      adc:
        controller: 'ads1115'
        enable: true
        i2c: 1
        devices: 1
        continuous: true
      arm1:
        controller: 'arm'
        enable: true
        shoulder:
          controller: 'servo'
          enable: true
          lpwm: 13
          rpwm: 26
          feedback:
            controller: 'potentiometer'
            enable: true
            adc: 'ads1115'
            id: 0
        upperarm:
          controller: 'linear'
          enable: false
          lpwm: 25
          rpwm: 24
          time: 5
          feedback:
            controller: 'potentiometer'
            enable: true
            adc: 'ads1115'
            id: 1
        forearm:
          controller: 'linear'
          enable: false
          lpwm: 18
          rpwm: 17
          time: 4
          feedback:
            controller: 'potentiometer'
            enable: true
            adc: 'ads1115'
            id: 2
        trigger:
          controller: 'solenoid'
          enable: false
          output: 23
    </rosparam>
  </node>
</launch>
