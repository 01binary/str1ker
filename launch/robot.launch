<launch>
  <node
    name="robot"
    pkg="str1ker"
    type="robot"
    required="true"
    output="screen"
  >
    <rosparam>
      adc:
        controller: 'ads1115'
        enable: true
        i2c: 1
        devices: 2
        rate: 64
      arm1:
        controller: 'arm'
        enable: true
        shoulder:
          controller: 'servo'
          enable: true
          gpioLPWM: 13
          gpioRPWM: 26
          encoder:
            controller: 'potentiometer'
            enable: true
            adc: 'ads1115'
            channel: 0
        upperarm:
          controller: 'linear'
          enable: false
          gpioLPWM: 25
          gpioRPWM: 24
          time: 5
          encoder:
            controller: 'potentiometer'
            enable: false
            adc: 'ads1115'
            channel: 1
        forearm:
          controller: 'linear'
          enable: false
          gpioLPWM: 18
          gpioRPWM: 17
          time: 4
          encoder:
            controller: 'potentiometer'
            enable: false
            adc: 'ads1115'
            channel: 2
        trigger:
          controller: 'solenoid'
          enable: false
          gpio: 23
    </rosparam>
  </node>
</launch>
