<launch>
  <!-- Serial bridge -->
  <include file="$(find str1ker)/launch/hardware.launch" />

  <!-- ROS control stack -->
  <include file="$(find str1ker)/launch/controllers.launch" />

  <!-- Robot controller -->
  <node
    name="robot"
    pkg="str1ker"
    type="robot"
    required="true"
    output="screen"
  />

</launch>
