            <mass value="0.703" />
            <inertia ixx="0.047" ixy="0.001" ixz="0.001" iyy="0.001" iyz="0.001" izz="0.047" />
        </inertial>
    </link>
    <link name="drumstick_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/Drumstick.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/DrumstickBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.091" />
            <inertia ixx="0.001" ixy="0.001" ixz="0.001" iyy="0.001" iyz="0.001" izz="0.001" />
        </inertial>
    </link>
    <link name="tip_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.01" />
            </geometry>
            <material name="transparent">
                <color rgba="1 1 1 0.1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.01" />
            </geometry>
        </collision>
    </link>
    <!-- Arm actuator links -->
    <link name="upperarm_actuator_body_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
