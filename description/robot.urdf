<?xml version="1.0" ?>
<robot name="str1ker">
    <link name="world" />
    <!-- Arm links -->
    <link name="base_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/ArmBase.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/ArmBaseBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.528" />
            <inertia ixx="0.001" ixy="1e-10" ixz="-0.001" iyy="0.005" iyz="1e-10" izz="0.004" />
        </inertial>
    </link>
    <link name="shoulder_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/Shoulder.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/ShoulderBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.36378108" />
            <inertia ixx="0.002" ixy="0.001" ixz="0.001" iyy="0.001" iyz="0.001" izz="0.001" />
        </inertial>
    </link>
    <link name="upperarm_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/UpperArm.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/UpperArmBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="1.139" />
            <inertia ixx="0.061" ixy="0.001" ixz="0.001" iyy="0.001" iyz="0.001" izz="0.060" />
        </inertial>
    </link>
    <link name="forearm_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/Forearm.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/ForearmBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.703" />
            <inertia ixx="0.047" ixy="0.001" ixz="0.001" iyy="0.001" iyz="0.001" izz="0.047" />
        </inertial>
    </link>
    <link name="drumstick_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/Drumstick.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/DrumstickBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.091" />
            <inertia ixx="0.001" ixy="0.001" ixz="0.001" iyy="0.001" iyz="0.001" izz="0.001" />
        </inertial>
    </link>
    <link name="tip_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.01" />
            </geometry>
            <material name="transparent">
                <color rgba="1 1 1 0.1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.01" />
            </geometry>
        </collision>
    </link>
    <!-- Arm joints -->
    <joint name="fixed" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0.06" />
        <parent link="world" />
        <child link="base_link" />
    </joint>
    <joint name="base" type="revolute">
        <origin rpy="1.5708 0 0" xyz="0 0 0.069" />
        <axis xyz="0 1 0" />
        <parent link="base_link" />
        <child link="shoulder_link" />
        <limit effort="100" velocity="1.0" lower="-1.4929" upper="1.4929" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>
    <joint name="shoulder" type="revolute">
        <origin rpy="0 0 0" xyz="-0.013 0.11518 0.0" />
        <axis xyz="0 0 1" />
        <parent link="shoulder_link" />
        <child link="upperarm_link" />
        <limit effort="100" velocity="1.0" lower="0.404676" upper="0.813453" />
        <dynamics damping="0.7" friction="1.4" />
        <mimic joint="upperarm_actuator" multiplier="7.640691589" offset="0.817273346" />
    </joint>
    <joint name="elbow" type="revolute">
        <origin rpy="0 0 0" xyz="0.4173 0 0" />
        <axis xyz="0 0 1" />
        <parent link="upperarm_link" />
        <child link="forearm_link" />
        <limit effort="100" velocity="1.0" lower="-1.93302196" upper="-1.123992" />
        <dynamics damping="0.7" friction="1.4" />
        <mimic joint="forearm_actuator" multiplier="15.925786614" offset="-1.123992" />
    </joint>
    <joint name="wrist" type="revolute">
        <origin rpy="0 0 0" xyz="0.48059393086 0 -0.023" />
        <axis xyz="0 0 1" />
        <parent link="forearm_link" />
        <child link="drumstick_link" />
        <limit effort="100" velocity="1.0" lower="0.959931" upper="0.959931" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>
    <joint name="tip" type="fixed">
        <origin rpy="0 0 0" xyz="0.305 0.0 0.0" />
        <parent link="drumstick_link" />
        <child link="tip_link" />
    </joint>
    <!-- Arm transmissions -->
    <transmission name="trans_base">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="base">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
        <actuator name="base_motor">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <!-- Arm controllers-->
    <gazebo>
        <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
            <robotNamespace>/</robotNamespace>
        </plugin>
    </gazebo>
</robot>
