                <mesh filename="package://str1ker/description/models/UpperArmActuatorBody.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/UpperArmActuatorBodyBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.952544" />
            <inertia ixx="0.0084" ixy="0.001" ixz="0.001" iyy="0.0084" iyz="-0.0012" izz="0.0012" />
        </inertial>
    </link>

    <gazebo reference="upperarm_actuator_body_link">
        <turnGravityOff>true</turnGravityOff>
        <selfCollide>false</selfCollide>
    </gazebo>
   
