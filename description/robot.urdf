<?xml version="1.0" ?>
<robot name="str1ker">
    <!-- Arm links -->

    <link name="world" />

    <link name="base_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/ArmBase.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/ArmBaseBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>
    </link>

    <link name="shoulder_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/Shoulder.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/ShoulderBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.36378108" />
            <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001" />
        </inertial>
    </link>

    <link name="upperarm_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/UpperArm.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/UpperArmBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="1.139" />
            <inertia ixx="0.061" ixy="0" ixz="0" iyy="0.001" iyz="0.001" izz="0.060" />
        </inertial>
    </link>

    <link name="forearm_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/Forearm.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/ForearmBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.703" />
            <inertia ixx="0.047" ixy="0" ixz="0" iyy="0" iyz="0.001" izz="0.047" />
        </inertial>
    </link>

    <link name="drumstick_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/Drumstick.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/DrumstickBoundingBox.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.091" />
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0" iyz="0" izz="0.001" />
        </inertial>
    </link>

    <!-- Arm actuator links -->

    <link name="upperarm_actuator_body_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/UpperArmActuatorBody.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/TODO.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.796" />
            <inertia ixx="0.007" ixy="0" ixz="0" iyy="0.007" iyz="-0.001" izz="0.001" />
        </inertial>
    </link>

    <link name="upperarm_actuator_shaft_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/UpperArmActuatorShaft.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/TODO.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="0.070" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>

    <link name="forearm_actuator_body_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/ForearmActuatorBody.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/TODO.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="0.199" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0" iyz="0" izz="0.001" />
        </inertial>
    </link>

    <link name="forearm_actuator_shaft_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/ForearmActuatorShaft.stl" />
            </geometry>
            <material name="aluminum">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://str1ker/description/models/TODO.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="0.113" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0" iyz="0" izz="0.001" />
        </inertial>
    </link>

    <!-- Arm joints -->

    <joint name="fixed" type="fixed">
        <parent link="world" />
        <child link="base_link" />
    </joint>

    <joint name="mount" type="revolute">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <axis xyz="0 0 1" />
        <parent link="base_link" />
        <child link="shoulder_link" />
        <limit effort="100" velocity="1.0" lower="-1.4929" upper="1.4929" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>

    <joint name="shoulder" type="revolute">
        <origin rpy="0 0 0" xyz="0 -0.0127 0.108839" />
        <axis xyz="1 0 0" />
        <parent link="shoulder_link" />
        <child link="upperarm_link" />
        <limit effort="100" velocity="1.0" lower="0" upper="0.785398" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>

    <joint name="elbow" type="revolute">
        <origin rpy="0 0 0" xyz="0 0.4173 0" />
        <axis xyz="1 0 0" />
        <parent link="upperarm_link" />
        <child link="forearm_link" />
        <limit effort="100" velocity="1.0" lower="-1.8" upper="-0.9" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>

    <joint name="wrist" type="revolute">
        <origin rpy="0 0 0" xyz="0 0.48059393086 0" />
        <axis xyz="1 0 0" />
        <parent link="forearm_link" />
        <child link="drumstick_link" />
        <limit effort="100" velocity="1.0" lower="1.2" upper="1.3" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>

    <!-- Arm actuator joints -->

    <joint name="shoulder_actuator_base" type="revolute ">
        <origin rpy="0 0 0" xyz="0 0.101 -0.057" />
        <axis xyz="1 0 0" />
        <parent link="shoulder_link" />
        <child link="upperarm_actuator_body_link" />
        <limit effort="100" velocity="1.0" lower="0" upper="2" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>

    <joint name="shoulder_actuator_shaft" type="prismatic">
        <origin rpy="0 0 0" xyz="0 -0.002 0.184" />
        <axis xyz="0 0 1" />
        <parent link="upperarm_actuator_body_link" />
        <child link="upperarm_actuator_shaft_link" />
        <limit effort="100" velocity="1.0" lower="0" upper="2" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>

    <joint name="shoulder_actuator_shaft_end" type="revolute ">
        <origin rpy="0 0 0" xyz="0 0 0.083" />
        <axis xyz="1 0 0" />
        <parent link="upperarm_actuator_shaft_link" />
        <child link="upperarm_link" />
        <limit effort="100" velocity="1.0" lower="0" upper="2" />
        <dynamics damping="0.7" friction="1.4" />
        <!-- connects to upperarm at +0.11 Y and -0.054 Z-->
    </joint>

    <joint name="forearm_actuator_base" type="revolute ">
        <origin rpy="0 0 0" xyz="0 0.130 -0.034" />
        <axis xyz="1 0 0" />
        <parent link="upperarm_link" />
        <child link="forearm_actuator_body_link" />
        <limit effort="100" velocity="1.0" lower="0" upper="2" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>

    <joint name="forearm_actuator_shaft" type="prismatic">
        <origin rpy="0 0 0" xyz="0 0 0.135" />
        <axis xyz="0 0 1" />
        <parent link="forearm_actuator_body_link" />
        <child link="forearm_actuator_shaft_link" />
        <limit effort="100" velocity="1.0" lower="0" upper="2" />
        <dynamics damping="0.7" friction="1.4" />
    </joint>

    <joint name="forearm_actuator_shaft_end" type="revolute ">
        <origin rpy="0 0 0" xyz="0 0 0.137" />
        <axis xyz="1 0 0" />
        <parent link="forearm_actuator_shaft_link" />
        <child link="forearm_link" />
        <limit effort="100" velocity="1.0" lower="0" upper="2" />
        <dynamics damping="0.7" friction="1.4" />
        <!-- Connects to forearm at +0.069 Y, -0.044 Z -->
    </joint>

    <!-- Arm transmissions -->

    <transmission name="trans_mount">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="mount">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
        <actuator name="mount_motor">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <transmission name="trans_shoulder">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="shoulder">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
        <actuator name="shoulder_motor">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <transmission name="trans_elbow">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="elbow">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
        <actuator name="elbow_motor">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <transmission name="trans_wrist">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="wrist">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        </joint>
        <actuator name="wrist_motor">
            <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <gazebo>
        <plugin name="gazebo_ros_control">
            <robotNamespace>/</robotNamespace>
        </plugin>
    </gazebo>
</robot>

